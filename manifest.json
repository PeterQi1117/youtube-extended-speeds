{
  "manifest_version": 3,
  "name": "YouTube Extended Speeds",
  "version": "1.0",
  "description": "Add 2.5x and 3x playback speeds to YouTube's speed menu.",
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "48": "icon48.png"
  }
}
